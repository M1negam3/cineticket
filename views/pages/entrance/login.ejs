<div id="login" class="content-container p-5">
  <h1 class="text-center headlines">In Account einloggen</h1>
  <div class="container-fluid pt-5 pb-5 movies-list-container">
    <div style="max-width: 450px;" class="mx-auto">
      <form action="login" method="POST">
        <div class="form-group pt-5 pb-5">
          <input type="email" name="emailAddress" class="form-control" placeholder="Email address" autocomplete="email"
            focus-first>
          <div class="invalid-feedback">Bitte eine gÃ¼ltige E-Mail Adresse eingeben.</div>
        </div>
        <div class="form-group pt-5 pb-5">
          <input name="password" type="password" class="form-control" placeholder="Password"
            autocomplete="current-password">
          <div class="invalid-feedback">Bitte Passwort eingeben.</div>
        </div>
        <!-- <span>Accountdaten konnten nicht gefunden werden.</span> -->
        <div class="form-group pt-5 pb-5">
          <button type="submit" class="btn btn-primary">Einloggen</button>
        </div>
      </form>
    </div>
  </div>
</div>
<%- /* Expose locals as `window.SAILS_LOCALS` :: */ exposeLocalsToBrowser() %>