<div class="content-container p-5 d-flex justify-content-center">
    <div class="w-75">
        <h2>Suchergebnis</h2>

        <div class="pt-3">
            <table class="table table-striped table-hover table-bordered">
                <thead>
                    <th>Name</th>
                    <th>Dauer</th>
                </thead>
                <tbody>
                    <% entries.forEach(function(entry){ %>
                        <tr>
                            <td>
                                <%= entry.name %>
                            </td>
                            <td>
                                <%= entry.duration %>
                            </td>
                            <td>
                                <%= entry.description %>
                            </td>
                        </tr>
                        <% }); %>
                </tbody>
            </table>
        </div>
        <div>
            <a class="btn btn-primary mt-3" href="/search">Neue Suche</a>
        </div>
    </div>
</div>