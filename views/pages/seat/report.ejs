<div id="app">
  <router-view></router-view>
</div>

<script type="module">

  import SeatReport from '/vue/seat-report.js'; 
  import AddSeatComponent from '/vue/add-seat.js';

  const routes = [
    { path: '/', redirect: '/seat/report' },
    { path: '/seat/report', component: SeatReport },
    {path: '/add-seat', component: AddSeatComponent},

  ];


  const router = VueRouter.createRouter({
    history: VueRouter.createWebHashHistory(),
    routes,
  });


  const app = Vue.createApp({});
  app.use(router);
  app.mount('#app');
</script>
