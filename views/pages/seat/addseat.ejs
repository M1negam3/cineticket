<div id="app">
    <router-view></router-view>
  </div>
  
  <script type="module">
  
    import SeatAdd from '/vue/seat-add.js'; 
  
    const routes = [
      { path: '/', redirect: '/seat/add' },
      { path: '/seat/add', component: SeatAdd }, 
    ];
  
  
    const router = VueRouter.createRouter({
      history: VueRouter.createWebHashHistory(),
      routes,
    });
  
  
    const app = Vue.createApp({});
    app.use(router);
    app.mount('#app');
  </script>